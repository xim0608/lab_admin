<%= form_for @user, :url => users_registrations_path do |f| %>
  <%= f.label :email %>
  <%# errors_for @user, :email %>
  <%= f.text_field :email, autofocus: true %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "登録" %>

<% end %>
