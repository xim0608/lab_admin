<!DOCTYPE html>
<html>
<head>
  <title>LabAdmin</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <div>
    <% flash.each do |type,msg| %>
      <p class="alert alert-<%= type %>">
        <%= msg %>
      </p>
    <% end %>
  </div>
  <div class="container">
    <%= yield %>
  </div>
</body>
</html>
